<identifier> ::= "[a-zA-Z_]+[a-zA-Z_0-9]*"
<strings> ::= "(?<=")[^\\\"\n]*.(?=")" | ""
<integer> ::= "0" | "0(x|X)[0-9A-F]+" | "[1-9]+[0-9]*" | "0+[0-7]+"
<float> ::= "[0-9eE]*\.?[0-9eE-]*"
<operator> ::= "\+" | "\-" | "\*" | "\/" | "%" | "=" | "\+=" | "\-=" | "\*=" | "<" | ">" | "--" | "<=" | ">=" | "&" | "==" | "!=" | "++" | "&&" | "\|\|"
